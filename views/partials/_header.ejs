<header>  
  <div>
    <h1>TinyApp</h1>
  </div>
  <div> 
    <% if (user) { %>
      <h4>Logged in as <%=user.email %></h4>
      <form action="/logout" method="post">
        <input type="submit" value="Logout">
      </form>
    <% } else { %>
      <ul>
        <li><a href="/login">Login</a></li>
        <li><a href="/register">Register</a></li>
      </ul>
      
     
    <% } %>
  </div>
</header>